
#include <stdio.h>

int main()
{
    int a,b,sum,sub,mul,div,mod;
    scanf("%d %d",&a,&b);
    sum = a + b;
    printf("%d ",sum);
    sub = a - b;
     printf("%d ",sub);
    mul = a * b;
     printf("%d ",mul);
    div = a / b;
     printf("%d ",div);
    mod = a % b;
     printf("%d ",mod);

    return 0;
}
